<h1>See your meetings!</h1>

<% @meetings.each do |meeting| %>

<p><%= link_to 'Meeting Date:', user_meeting_path(current_user, meeting) %> <%= meeting.created_at.strftime("%B %d, %Y")%><br>
Restaurant: <%= Restaurant.find(meeting.restaurant_id).name %><br>
Guest: <%= meeting.users.where.not(id: params[:user_id]).first.name%>
</p>
<% end %>
  